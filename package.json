{
  "name": "js-webpack3",
  "version": "1.0.0",
  "description": "Learning Webpack 3",
  "main": "src/app.js",
  "scripts": {
    "build:development": "cross-env NODE_ENV=development webpack -d",
    "build:production": "cross-env NODE_ENV=production webpack -p --progress --profile --colors",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress --profile --colors",
    "build:multiple": "webpack --config webpack.multiple.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Angga Ari Wijaya",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^1.5.0-beta.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4"
  }
}
